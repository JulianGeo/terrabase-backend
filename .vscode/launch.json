{
  "version": "0.2.0",
  "configurations": [
    {
      "env": {
        "STAGE": "DEV"
      },
      "type": "node",
      "request": "launch",
      "name": "Debug Nest Framework",
      "args": [
        "${workspaceFolder}/src/main.ts"
      ],
      "runtimeArgs": [
        "--nolazy",
        "-r",
        "ts-node/register",
        "-r",
        "tsconfig-paths/register"
      ],
      "sourceMaps": true,
      "envFile": "${workspaceFolder}/.env.stage.env",
      "cwd": "${workspaceRoot}",
      "console": "integratedTerminal",
      //"preLaunchTask": "Run Prisma Migration"
    }
  ]
}